import java.util.Arrays;

public class Test3 {
	public static void main(String[] args) {

		int [] Arr = {9,6,7,3,5};
		for(int i=0; i<Arr.length; i++) {
			int min =i;
			for(int j=0; j<Arr.length; j++) {
			if(Arr[j]<Arr[min]) {
					min = j;
			}
			}
				min=Arr[i];
				Arr[i]=Arr[i+1];
				Arr[i+1]=min;
			
		System.out.println((i+1) + "회전" + Arrays.toString(Arr));
		
	}
}
}		