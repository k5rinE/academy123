public class Ex0 {

	public static void main(String[] args) {

		int[][] arr = {
					  { 5, 5, 5, 5, 5},
					  {10,10,10,10,10},
				   	  {20,20,20,20,20},
					  {30,30,30,30,30}
					  };
		int total = 0;
		int total1 = 0;
		int total2 = 0;
		int total3 = 0;
		int total4 = 0;
		int total5 = 0;
		float average = 0;
		
		for(int i=0; i<arr.length; i++) {
				total1 += arr[i][0];
				total2 += arr[i][1];
				total3 += arr[i][2];
				total4 += arr[i][3];
				total5 += arr[i][4];
				for(int j=0; j<arr[i].length;j++) {
				total +=arr[i][j];
				}
		}
				
		
		System.out.printf("total= %d",total);
				
		
		
		
		//System.out.println("average= " + average);
	}
}